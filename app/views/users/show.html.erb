<% provide(:title, "ToDo List | " + @user.name)%>
<h1>Bienvenido <%= @user.name %></h1>
<ul>
<% @list.each do |item| %>
  <li>
    <input id="task_<%= item.id %>" type="checkbox"<% if item.completed? %> checked<% end %> onclick="completeTask(<%= item.id %>)"><%= item.title %></input>
  </li>
<% end %>
</ul>
<button class="btn btn-primary js-add-task">Add Task</button>